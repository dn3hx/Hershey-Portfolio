      "source": [
        "import torch\n",
        "import torch.nn as nn\n",
        "import torch.nn.functional as F\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "torch.manual_seed(42)\n",
        "\n",
        "# -----------------------------\n",
        "# Simulated Inputs\n",
        "# -----------------------------\n",
        "\n",
        "seq_len = 5\n",
        "hidden_size = 8\n",
        "metadata_size = 4\n",
        "\n",
        "encoder_hidden = torch.randn(seq_len, hidden_size)\n",
        "decoder_state = torch.randn(hidden_size)\n",
        "\n",
        "metadata_vector = torch.tensor([0.0, 3.0, 1.0, 1.0])\n",
        "\n",
        "# -----------------------------\n",
        "# Original Bahdanau Attention\n",
        "# -----------------------------\n",
        "\n",
        "class OriginalBahdanau(nn.Module):\n",
        "    def __init__(self, hidden_size):\n",
        "        super().__init__()\n",
        "        self.W_s = nn.Linear(hidden_size, hidden_size)\n",
        "        self.W_h = nn.Linear(hidden_size, hidden_size)\n",
        "        self.v = nn.Linear(hidden_size, 1)\n",
        "\n",
        "    def forward(self, decoder_state, encoder_hidden):\n",
        "        scores = []\n",
        "        for h_i in encoder_hidden:\n",
        "            score = self.v(\n",
        "                torch.tanh(self.W_s(decoder_state) + self.W_h(h_i))\n",
        "            )\n",
        "            scores.append(score)\n",
        "        scores = torch.stack(scores).squeeze()\n",
        "        return F.softmax(scores, dim=0)\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Enhanced Bahdanau Attention\n",
        "# -----------------------------\n",
        "\n",
        "class EnhancedBahdanau(nn.Module):\n",
        "    def __init__(self, hidden_size, metadata_size):\n",
        "        super().__init__()\n",
        "        self.W_s = nn.Linear(hidden_size, hidden_size)\n",
        "        self.W_h = nn.Linear(hidden_size, hidden_size)\n",
        "        self.W_m = nn.Linear(metadata_size, hidden_size)\n",
        "        self.v = nn.Linear(hidden_size, 1)\n",
        "\n",
        "    def forward(self, decoder_state, encoder_hidden, metadata):\n",
        "        scores = []\n",
        "        for h_i in encoder_hidden:\n",
        "            score = self.v(\n",
        "                torch.tanh(\n",
        "                    self.W_s(decoder_state) +\n",
        "                    self.W_h(h_i) +\n",
        "                    self.W_m(metadata)\n",
        "                )\n",
        "            )\n",
        "            scores.append(score)\n",
        "        scores = torch.stack(scores).squeeze()\n",
        "        return F.softmax(scores, dim=0)\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Run Models\n",
        "# -----------------------------\n",
        "\n",
        "original_model = OriginalBahdanau(hidden_size)\n",
        "enhanced_model = EnhancedBahdanau(hidden_size, metadata_size)\n",
        "\n",
        "original_attention = original_model(decoder_state, encoder_hidden).detach().numpy()\n",
        "enhanced_attention = enhanced_model(decoder_state, encoder_hidden, metadata_vector).detach().numpy()\n",
        "\n",
        "print(\"Original Attention:\", original_attention)\n",
        "print(\"Enhanced Attention:\", enhanced_attention)\n",
        "\n",
        "# -----------------------------\n",
        "# Visualization\n",
        "# -----------------------------\n",
        "\n",
        "tokens = [\"T1\", \"T2\", \"T3\", \"T4\", \"T5\"]\n",
        "x = np.arange(len(tokens))\n",
        "width = 0.35\n",
        "\n",
        "# Bar Chart Comparison\n",
        "plt.figure(figsize=(8,5))\n",
        "plt.bar(x - width/2, original_attention, width, label='Original')\n",
        "plt.bar(x + width/2, enhanced_attention, width, label='Enhanced')\n",
        "plt.xticks(x, tokens)\n",
        "plt.ylabel(\"Attention Weight\")\n",
        "plt.title(\"Original vs Enhanced Bahdanau Attention\")\n",
        "plt.legend()\n",
        "plt.show()\n",
        "\n",
        "# Difference Graph\n",
        "difference = enhanced_attention - original_attention\n",
        "\n",
        "plt.figure(figsize=(8,5))\n",
        "plt.bar(tokens, difference)\n",
        "plt.ylabel(\"Weight Difference\")\n",
        "plt.title(\"Attention Shift After Metadata Injection\")\n",
        "plt.axhline(0)\n",
        "plt.show()\n"
      ]
    }
  ]
}